    <FactionDef ParentName="VFEM_UnknownKingdomBase">
        <defName>VFEM_UnknownKingdom</defName>
    </FactionDef>

    <FactionDef ParentName="VFEM_UnknownKingdomBase">
        <defName>VFEM_UnknownKingdomHostile</defName>
        <permanentEnemy>true</permanentEnemy>
    </FactionDef>

