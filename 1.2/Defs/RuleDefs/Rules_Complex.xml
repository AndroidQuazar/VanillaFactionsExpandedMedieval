<?xml version="1.0" encoding="utf-8"?>
<Defs>

    <RuleDef>
        <defName>VFEM_MedievalSettlement</defName>
        <symbol>VFEM_medievalSettlement</symbol>
        <resolvers>
            <li Class="VFEMedieval.SymbolResolver_MedievalSettlement">
                <minRectSize>(12, 12)</minRectSize>
            </li>
        </resolvers>
    </RuleDef>

    <RuleDef>
        <defName>VFEM_MedievalEdgeDefense</defName>
        <symbol>VFEM_medievalEdgeDefense</symbol>
        <resolvers>
            <li Class="VFEMedieval.SymbolResolver_MedievalEdgeDefense">
                <minRectSize>(2, 2)</minRectSize>
            </li>
        </resolvers>
    </RuleDef>

    <RuleDef>
        <defName>VFEM_Winery</defName>
        <symbol>VFEM_winery</symbol>
        <resolvers>
            <li Class="SymbolResolver_GenericRoom">
                <minRectSize>(4, 4)</minRectSize>
                <interior>VFEM_interior_winery</interior>
            </li>
        </resolvers>
    </RuleDef>

    <RuleDef>
        <defName>VFEM_AddMustToWineBarrels</defName>
        <symbol>VFEM_addMustToWineBarrels</symbol>
        <resolvers>
            <li Class="VFEMedieval.SymbolResolver_AddMustToWineBarrels" />
        </resolvers>
    </RuleDef>

</Defs>