<?xml version="1.0" encoding="utf-8"?>
<Defs>
	<ThingDef ParentName="BuildingBase">
		<defName>VFEM_WineBarrel</defName>
		<label>wine barrel</label>
		<description>A barrel for fermenting raw must into wine.</description>
		<thingClass>ItemProcessor.Building_ItemProcessor</thingClass>
		<drawerType>MapMeshAndRealTime</drawerType>
		<graphicData>
			<texPath>Things/Building/Production/WineBarrel/WineBarrel</texPath>
			<graphicClass>Graphic_Multi</graphicClass>
			<damageData>
				<rect>(0.05,0.1,0.9,0.9)</rect>
			</damageData>
			<shadowData>
				<volume>(0.75, 0.35, 0.33)</volume>
			</shadowData>
		</graphicData>
		<minifiedDef>MinifiedThing</minifiedDef>
		<altitudeLayer>Building</altitudeLayer>
		<passability>PassThroughOnly</passability>
		<fillPercent>0.45</fillPercent>
		<pathCost>60</pathCost>
		<thingCategories>
			<li>BuildingsProduction</li>
		</thingCategories>
		<statBases>
			<WorkToBuild>600</WorkToBuild>
			<Mass>10</Mass>
			<MaxHitPoints>100</MaxHitPoints>
			<Flammability>1.0</Flammability>
		</statBases>
		<costList>
			<Steel>10</Steel>
			<WoodLog>30</WoodLog>
		</costList>
		<comps>
			<li Class="ItemProcessor.CompProperties_ItemProcessor">
				<buildingOnGraphic>Things/Building/Production/WineBarrel/WineBarrel</buildingOnGraphic>
				<numberOfInputs>1</numberOfInputs>
				<qualitySelector>true</qualitySelector>
				<acceptsNoneAsInput>false</acceptsNoneAsInput>
				<InsertFirstItemDesc>VanillaFactionsExpanded.Wine.InsertFirstItemDesc</InsertFirstItemDesc>
				<isSemiAutomaticMachine>true</isSemiAutomaticMachine>
				<resetSemiautomaticIcon>UI/MustReset</resetSemiautomaticIcon>
				<chooseIngredientsIcon>UI/MustEmpty</chooseIngredientsIcon>
				<showProgressBar>true</showProgressBar>
				<bringIngredientsText>VanillaFactionsExpanded.Wine.bringIngredientsText</bringIngredientsText>
				<bringIngredientsDesc>VanillaFactionsExpanded.Wine.bringIngredientsDesc</bringIngredientsDesc>
				<bringIngredientsIcon>UI/MustInsert</bringIngredientsIcon>
				<cancelIngredientsText>VanillaFactionsExpanded.Wine.cancelIngredientsText</cancelIngredientsText>
				<cancelIngredientsDesc>VanillaFactionsExpanded.Wine.cancelIngredientsDesc</cancelIngredientsDesc>
				<cancelIngredientsIcon>UI/MustCancel</cancelIngredientsIcon>
				<noPowerDestroysProgress>false</noPowerDestroysProgress>
				<removeProductText>VanillaFactionsExpanded.Wine.removeProductText</removeProductText>
				<removeProductDesc>VanillaFactionsExpanded.Wine.removeProductDesc</removeProductDesc>
				<removeProductIcon>UI/RemoveWine</removeProductIcon>
				<isTemperatureDependingMachine>true</isTemperatureDependingMachine>
				<minTemp>-1</minTemp>
				<maxTemp>32</maxTemp>
				<destroyIngredientsAtStartOfProcess>false</destroyIngredientsAtStartOfProcess>
				<destroyIngredientsAtAwfulQuality>true</destroyIngredientsAtAwfulQuality>
			</li>
		</comps>
		<tickerType>Rare</tickerType>
		<rotatable>true</rotatable>
		<designationCategory>Production</designationCategory>
		<constructEffect>ConstructWood</constructEffect>
		<researchPrerequisites>
			<li>Brewing</li>
		</researchPrerequisites>
	</ThingDef>
	<ThingDef ParentName="BuildingBase">
		<defName>VFEM_MeatSmoker</defName>
		<label>meat smoker</label>
		<description>A small wooden box that needs to be refueled with wood periodically. Meat of any kind gets inserted into it and then gets smoked to last longer.</description>
		<thingClass>ItemProcessor.Building_ItemProcessor</thingClass>
		<graphicData>
			<texPath>Things/Building/Production/MeatSmokerEmpty/MeatSmokerEmpty</texPath>
			<graphicClass>Graphic_Multi</graphicClass>
			<drawSize>(1.4,1.4)</drawSize>
		</graphicData>
		<uiIconPath>Things/Building/Production/MeatSmokerEmpty/MeatSmoker_MenuIcon</uiIconPath>
		<altitudeLayer>Building</altitudeLayer>
		<statBases>
			<WorkToBuild>1200</WorkToBuild>
		</statBases>
        <researchPrerequisites>
          <li>VFEM_MeatSmoking</li>
        </researchPrerequisites>
		<tickerType>Normal</tickerType>
		<scatterableOnMapGen>false</scatterableOnMapGen>
		<useHitPoints>false</useHitPoints>
		<size>(1,1)</size>
		<designationCategory>Production</designationCategory>
		<passability>Standable</passability>
		<hasInteractionCell>True</hasInteractionCell>
		<interactionCellOffset>(0,0,-1)</interactionCellOffset>
		<surfaceType>Item</surfaceType>
		
		<costList>
			<Steel>10</Steel>
			<WoodLog>20</WoodLog>
		</costList>
		<building>
			<spawnedConceptLearnOpportunity>BillsTab</spawnedConceptLearnOpportunity>
			<sowTag>SupportPlantsOnly</sowTag>
			<canPlaceOverImpassablePlant>false</canPlaceOverImpassablePlant>
			<ai_chillDestination>false</ai_chillDestination>
			<artificialForMeditationPurposes>false</artificialForMeditationPurposes>
			<buildingTags>
				<li>Production</li>
			</buildingTags>
		</building>
		<comps>
			<li Class="ItemProcessor.CompProperties_ItemProcessor">
				<buildingOnGraphic>Things/Building/Production/MeatSmokerFull/MeatSmokerFull</buildingOnGraphic>
				<numberOfInputs>1</numberOfInputs>
				<qualitySelector>false</qualitySelector>
				<isCategoryBuilding>true</isCategoryBuilding>
				<InsertFirstItemDesc>VanillaFactionsExpanded.Smoker.InsertFirstItemDesc</InsertFirstItemDesc>
				<isSemiAutomaticMachine>true</isSemiAutomaticMachine>
				<resetSemiautomaticIcon>UI/VFEM_MeatReset</resetSemiautomaticIcon>
				<chooseIngredientsIcon>UI/VFEM_MeatEmpty</chooseIngredientsIcon>
				<showProgressBar>true</showProgressBar>
				<bringIngredientsText>VanillaFactionsExpanded.Smoker.bringIngredientsText</bringIngredientsText>
				<bringIngredientsDesc>VanillaFactionsExpanded.Smoker.bringIngredientsDesc</bringIngredientsDesc>
				<bringIngredientsIcon>UI/VFEM_InsertMeat</bringIngredientsIcon>
				<cancelIngredientsText>VanillaFactionsExpanded.Smoker.cancelIngredientsText</cancelIngredientsText>
				<cancelIngredientsDesc>VanillaFactionsExpanded.Smoker.cancelIngredientsDesc</cancelIngredientsDesc>
				<cancelIngredientsIcon>UI/VFEM_CancelMeat</cancelIngredientsIcon>
				<noPowerDestroysProgress>true</noPowerDestroysProgress>
				<destroyIngredientsAtStartOfProcess>true</destroyIngredientsAtStartOfProcess>
			</li>
			<li Class="CompProperties_Refuelable">
				<fuelConsumptionRate>3.33</fuelConsumptionRate>
				<fuelCapacity>20.0</fuelCapacity>
				<fuelConsumptionPerTickInRain>0.0006</fuelConsumptionPerTickInRain>
				<fuelFilter>
					<thingDefs>
						<li>WoodLog</li>
					</thingDefs>
				</fuelFilter>
				<initialFuelPercent>1</initialFuelPercent>
				<showAllowAutoRefuelToggle>true</showAllowAutoRefuelToggle>
				<consumeFuelOnlyWhenUsed>false</consumeFuelOnlyWhenUsed>
			</li>
		</comps>
	</ThingDef>
</Defs>