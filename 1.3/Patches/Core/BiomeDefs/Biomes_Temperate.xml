<?xml version="1.0" encoding="utf-8"?>
<Patch>

    <!-- Integration with RimCuisine 2 -->
    <Operation Class="PatchOperationFindMod">
        <mods>
            <li>[1.0] RimCuisine 2 Core</li>
        </mods>
        <match Class="PatchOperationSequence">
            <operations>
            
                <!-- Our grapes can spawn naturally -->
                <li Class="PatchOperationReplace">
                    <xpath>/Defs/BiomeDef[defName="TemperateForest"]/wildPlants/RC2_PlantGrapes</xpath>
                    <value>
                        <VFEM_Plant_Grape>0.0000000000000001</VFEM_Plant_Grape>
                    </value>
                </li>

            </operations>
        </match>
    </Operation>

</Patch>