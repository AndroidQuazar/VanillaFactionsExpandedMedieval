<?xml version="1.0" encoding="utf-8"?>
<Patch>

    <!-- If Editable Backstories is loaded, make the medieval factions use medieval backstories -->
    <Operation Class="PatchOperationFindMod">
        <mods>
            <li>[RF] Editable Backstories and Names - 1.1</li>
        </mods>
        <match Class="PatchOperationReplace">
            <xpath>/Defs/FactionDef[defName="VFEM_PlayerKingdom"]/backstoryCategories/li[text()="Tribal"]</xpath>
            <value>
                <li>Medieval</li>
            </value>
        </match>
    </Operation>

</Patch>