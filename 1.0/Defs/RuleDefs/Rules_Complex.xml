<?xml version="1.0" encoding="utf-8"?>
<Defs>

    <RuleDef>
        <defName>MedievalSettlement</defName>
        <symbol>medievalSettlement</symbol>
        <resolvers>
            <li Class="VFEMedieval.SymbolResolver_MedievalSettlement">
                <minRectSize>(12, 12)</minRectSize>
            </li>
        </resolvers>
    </RuleDef>

    <RuleDef>
        <defName>MedievalEdgeDefense</defName>
        <symbol>medievalEdgeDefense</symbol>
        <resolvers>
            <li Class="VFEMedieval.SymbolResolver_MedievalEdgeDefense">
                <minRectSize>(2, 2)</minRectSize>
            </li>
        </resolvers>
    </RuleDef>

    <RuleDef>
        <defName>Winery</defName>
        <symbol>winery</symbol>
        <resolvers>
            <li Class="SymbolResolver_GenericRoom">
                <minRectSize>(4, 4)</minRectSize>
                <interior>interior_winery</interior>
            </li>
        </resolvers>
    </RuleDef>

    <RuleDef>
        <defName>AddMustToWineBarrels</defName>
        <symbol>addMustToWineBarrels</symbol>
        <resolvers>
            <li Class="VFEMedieval.SymbolResolver_AddMustToWineBarrels" />
        </resolvers>
    </RuleDef>

</Defs>