<?xml version="1.0" encoding="utf-8"?>
<Defs>

    <RuleDef>
        <defName>VFE_MedievalSettlement</defName>
        <symbol>VFE_medievalSettlement</symbol>
        <resolvers>
            <li Class="VFEMedieval.SymbolResolver_MedievalSettlement">
                <minRectSize>(12, 12)</minRectSize>
            </li>
        </resolvers>
    </RuleDef>

    <RuleDef>
        <defName>VFE_MedievalEdgeDefense</defName>
        <symbol>VFE_medievalEdgeDefense</symbol>
        <resolvers>
            <li Class="VFEMedieval.SymbolResolver_MedievalEdgeDefense">
                <minRectSize>(2, 2)</minRectSize>
            </li>
        </resolvers>
    </RuleDef>

    <RuleDef>
        <defName>VFE_Winery</defName>
        <symbol>VFE_winery</symbol>
        <resolvers>
            <li Class="SymbolResolver_GenericRoom">
                <minRectSize>(4, 4)</minRectSize>
                <interior>interior_winery</interior>
            </li>
        </resolvers>
    </RuleDef>

    <RuleDef>
        <defName>VFE_AddMustToWineBarrels</defName>
        <symbol>VFE_addMustToWineBarrels</symbol>
        <resolvers>
            <li Class="VFEMedieval.SymbolResolver_AddMustToWineBarrels" />
        </resolvers>
    </RuleDef>

</Defs>